{
  "name": "mauzl-ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "build:export": "NEXT_EXPORT_BUILD=true next build --no-lint",
    "dev": "next dev",
    "lint": "next lint",
    "start": "next start",
    "test:watch": "jest --watch",
    "test:snap": "jest --maxWorkers=50% --silent --watchAll=false --testMatch='!**/*' --testMatch='**/*.snap.tsx' --passWithNoTests",
    "test:coverage": "jest --coverage --maxWorkers=50% --silent --watchAll=false --reporters=default --coverageReporters=cobertura --coverageReporters=text-summary --coverageReporters=json-summary --coverageReporters=lcov --testMatch='**/*.{spec,test}.{js,jsx,ts,tsx}' --passWithNoTests"
  },
  "dependencies": {
    "next": "14.2.8",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.2.8",
    "jest": "^29.7.0",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
